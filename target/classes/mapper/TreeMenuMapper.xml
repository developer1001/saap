    <!--<select id="findById" parameterType="int"-->
            <!--resultType="com.zgc.saap.model.User">-->
        <!--select * from t_user where id=#{id}-->
    <!--</select>-->
    <!-- 查询所有的菜单项 -->
    <select id="findAllObj" resultType="com.zgc.saap.model.TreeMenu">
        select * from sys_menu;
    </select>
</mapper>